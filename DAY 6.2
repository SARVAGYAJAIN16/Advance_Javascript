const fun3 = ()  => {
    setTimeout(() => { // it tell asynchronous behavior.. now code will go to web Api and wait for 2sec.
        // web api is not part of javascript. it provided by browser
        console.log("fun3 start and ends");
    },2000); // 2 sec after we get output here 
};

const fun4 = () => { // it will run first
   console.log("fun4 is start");
   fun3();
   console.log("fun4 ends");
};
fun4();
// output 
//fun4 is start
//fun4 ends
// after 2 sec ... next line will come
